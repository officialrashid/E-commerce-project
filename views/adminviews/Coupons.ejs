<% if (Coupons.length > 0) { %>
  <section id="footer" class="" style="margin-top: 5%;">
    <table class="table table-bordered datatable">
        <thead>
          <tr>
            <th scope="col">CouponCode</th>
            <th scope="col">CouponName</th>
            <th scope="col">Discount</th>
            <th scope="col">StartDate</th>
            <th scope="col">EndDate</th>
            <th scope="col">MinimumPrice</th>
            <th scope="col">MaxPrice</th>
            <th scope="col">Action</th>
          </tr>
        </thead>
        <tbody>
            <% Coupons.forEach(function(Coupon, i){ %>
          <tr >
        
          
            <td><%= Coupon.Code %></td>
            <td><%= Coupon.Name %></td>
            <td><%= Coupon.Discounts %></td>
            <td><%= Coupon.StartDates.toISOString().split('T')[0]; %></td>
            <td><%= Coupon.EndDates.toISOString().split('T')[0]; %></td>
            <td><%= Coupon.MinimumAmounts %></td>
            <td><%= Coupon.MaximumAmounts %></td>
           <td>
            <a href="/admin/editCoupon/<%= Coupon._id%>"><button class="badge bg-dark mb-3" id="my-button" onclick="myFunction(this.id)">Edit</button></a><br>
            <a href="/admin/deleteCoupon/<%= Coupon._id%>"><button class="badge bg-danger" onclick="return confirm('Are you sure you want to delete this file? This action cannot be undone.')",myFunction(this.id)>Remove</button></a>
           </td>
          </tr>
          <% }); %> 
        </tbody>
      </table>
    </section>
    <% } else { %>
      
   <% } %>
    