            document.addEventListener("DOMContentLoaded", () => {
              echarts.init(document.querySelector("#trafficChart")).setOption({
                tooltip: {
                  trigger: 'item'
                },
                legend: {
                  top: '5%',
                  left: 'center'
                },
                series: [{
                  name: 'Access From',
                  type: 'pie',
                  radius: ['40%', '70%'],
                  avoidLabelOverlap: false,
                  label: {
                    show: false,
                    position: 'center'
                  },
                  emphasis: {
                    label: {
                      show: true,
                      fontSize: '18',
                      fontWeight: 'bold'
                    }
                  },
                  labelLine: {
                    show: false
                  },
                  data: [{
                    value: <%= data.DELIVERED %>,
                    name: 'Delivered'
                  },
                  {
                    value:  <%= data.PENDING %>,
                    name: 'Pending'
                  },
                  {
                    value:  <%= data.CANCEL %>,
                    name: 'Cancel'
                  },
                  {
                    value:  <%= data.RETURN %>,
                    name: 'Return'
                  },

                  ]
                }]
              });
            });
          </script>

